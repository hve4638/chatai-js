{
    "name": "@hve/chatai",
    "version": "0.4.4",
    "main": "./dist/bundle.cjs",
    "module": "./dist/bundle.mjs",
    "types": "./dist/index.d.ts",
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/bundle.mjs",
            "require": "./dist/bundle.cjs",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "repository": "https://github.com/hve4638/chatai-ts.git",
    "author": "Hve <hve4638@gmail.com>",
    "license": "MIT",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "scripts": {
        "start": "ts-node ./playground/index.ts",
        "build": "rollup -c",
        "ts-build": "npm run ts-build-cjs && npm run ts-build-esm",
        "ts-build-cjs": "tsc --project tsconfig.cjs.json",
        "ts-build-esm": "tsc --project tsconfig.esm.json",
        "pub": "npm publish --access=public"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^28.0.1",
        "@rollup/plugin-json": "^6.1.0",
        "@rollup/plugin-node-resolve": "^15.3.0",
        "@rollup/plugin-typescript": "^12.1.1",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.10.2",
        "jest": "^29.7.0",
        "rollup": "^2.79.2",
        "rollup-plugin-terser": "^7.0.2",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.2"
    },
    "dependencies": {
        "axios": "^1.7.9",
        "tslib": "^2.8.1"
    }
}